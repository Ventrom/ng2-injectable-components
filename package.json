{
  "name": "ng2-injectable-components",
  "version": "0.0.0",
  "description": "A project to inject components into other Angular2 components",
  "main": "./dist/umd/ng2-injectable-components.js",
  "module": "./dist/esm/src/index.js",
  "typings": "./dist/esm/src/index.d.ts",
  "scripts": {
    "start": "concurrently --raw \"webpack-dev-server --open\"",
    "build:demo": "webpack -p",
    "build:umd": "webpack --config webpack.config.umd.js",
    "build:ngc": "ngc -p tsconfig-ngc.json",
    "build:dist": "npm run build:umd && npm run build:ngc",
    "build:clean": "rm -rf dist",
    "test": "karma start",
    "test:watch": "karma start --watch",
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s",
    "typedoc": "typedoc --options typedoc.json ng2-injectable-components.ts src/*.ts",
    "gh-pages": "git checkout gh-pages && git merge master && npm run build:demo && npm run typedoc && git add . && git commit -m 'chore: build demo and docs' && git push && git checkout master",
    "prerelease": "npm test",
    "release": "standard-version --first-release && git push --follow-tags origin master && npm run build:dist && npm publish",
    "postrelease": "npm run build:clean && npm run gh-pages"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Ventrom/Ventrom/ng2-injectable-components.git"
  },
  "keywords": [
    "angular2"
  ],
  "author": "Ventrom LLC",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Ventrom/Ventrom/ng2-injectable-components/issues"
  },
  "homepage": "https://github.com/Ventrom/Ventrom/ng2-injectable-components#readme",
